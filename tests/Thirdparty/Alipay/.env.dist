# 商户UID
alipay_pid=1111

# APPID
alipay_appid=2222

# 网关
alipay_gateway="https://openapi.alipay.com/gateway.do"

# 私钥路径
alipay_app_private_key_path="/path/alipay_key.pem"

# 公钥路径
alipay_public_key_path="/path/alipay_key.pub"

# 当面付(仅扫码付)异步通知地址
alipay_notify_url="https://github.com/farwish/alcon"

# 应用私钥字符(可选, 默认优先使用 alipay_app_private_key_path)
alipay_app_private_key=""

# 支付宝公钥字符(可选，默认优先使用 alipay_public_key_path)
alipay_public_key=""
